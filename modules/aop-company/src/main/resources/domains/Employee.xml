<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

  <module package="com.axelor.company.db" name="company"/>
  
  <entity name="Employee">
  	<string name="firstName"/>
  	<string name="lastName"/>
  	<string name="empName" title="Employee Name" >
 		<![CDATA[ return ("firstName" + " " + "lastName"]]>
 	</string>
  	<string name="designation"/>
  	<one-to-many ref="Department" name="deptEmpl" mappedBy="emplDept"/>
<!--   	<many-to-many ref="Employee" name="EmpProj"/> -->
<!--   	<many-to-one ref="Employee" name="EmpMgr" /> -->
  </entity>

</domain-models>